                    <script type="text/javascript">
    
    	index_lunbo();
    	function index_lunbo(){
            var len = $('.index_con_left_tg li').length;
            var soleW = 160;
            var soleH = 400;
            var num = 0;
            var timer = 0;
            var L = 0;
            var str ='';
    	
            //���ú��ӵ�ul li�ĸ�
            $('.index_con_left_tg ul li').css({
                height:soleH,
                width:soleW
            });
    
            //���ú��ӵ�ul��
            $('.index_con_left_tg ul').css({
                width:len*soleW
            });
    
            //���ú��ӵĸ�
            $('.index_con_left_tg').css({
                height:soleH
            });
            //��ʱ��	
    	    timer = setInterval(fn1,3000);
    
            //��������Ƴ�Ч��
            $('.index_con_left_tg').hover(function(){
                clearInterval(timer);
            },function(){
                timer = setInterval(fn1,3000);
            });
    
            function fn1() {
                num++;
                num %= len;
                L = (num) * soleW;
                $('.index_con_left_tg ul').animate({
                    'left': -L
                },300);
    			
                $('.dot').find('span').removeClass('active');
    			$('.dot').find('span').eq(num).addClass('active');
    		}
        };
