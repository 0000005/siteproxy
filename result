            function forwards(url)
            {
                if(url == "")
                    return;
                window.parent.location.href=url;
            }
            function spread(obj){
                if($(obj).attr("display") == "0"){
                  $(obj).next().children().removeAttr("style");
                  $(obj).children("img").attr("src","../img/icon_elves044.png");
                  $(obj).attr("display","1");
                }else{
                    $(obj).next().children().css("display","none");
                    $(obj).children("img").attr("src","../img/icon_elves033.png");
                    $(obj).attr("display","0");
                }
            }
            function spread_1(obj){
                if($(obj).attr("display") == "0"){
                    $(obj).children(".min_nav01_01_01_class").removeAttr("style");
                    $(obj).children(".min_nav01_01_01_class_1").children("img").attr("src","../img/icon_elves044.png");
                    $(obj).attr("display","1");
                }else{
                    $(obj).children(".min_nav01_01_01_class").css("display","none");
                    $(obj).children(".min_nav01_01_01_class_1").children("img").attr("src","../img/icon_elves033.png");
                    $(obj).attr("display","0");
                }
            }
        </script>
