        function searchByType(){
            var type=$('#selType').val();
            var info=$('#schInfo').val();
            if(info.length<1){
                alert("������ؼ���");
                return;
            }
            if(type=="site"){
                $("#site_search").attr('value',info);
                document.searchform.submit();
            }else if(type=="board"){
                $("#board_search").attr('value',info);
                document.searchform_brd.submit();
            }else if(type=="news"){
                if(info.length<2){
                    alert('����������������');
                    return;
                }else{
                    $("#news_search").attr('value',info);
                }
                document.searchform_kws.submit();
            }else if(type=="friend"){
                $("#user_search").attr('value',info);
                document.friendform.submit();
            }else if(type=="friend_name"){
                $("#username_search").attr('value',info);
                document.friendnameform.submit();
            }
            return;
        }
        function keyEvent(event){
            // �жϻس����Ƿ���
            if(!event)
                event = window.event;//������� window.event
            if((event.keyCode || event.which) != 13)
                return true;
    
            searchByType();
        }
    
    
    function getCookieHeader(name) {
    	var arr = document.cookie.split("; ");
    	var length = arr.length;
    	var a;
    	for (var i=0; i < length; i++) {
    		a = arr[i].split("=");
    		if (a[0].toLowerCase() == name.toLowerCase()) {
    			return a[1];
    		}
    	}
    
    	return "";
    }
    function GetUrl()
    {
          str=window.location.pathname;
          if(str==null)
          {
              var url = '/mwap';
          }
          else {
              var arr = new Array();
              arr = str.split("/");
              var url = '/mwap';
              if (arr.length == 1) {
                  url = '/mwap';
              }
              if(arr.length==2) {
                  if (arr[1] == "mitbbs_bbssec.php") {
                      url = '/mwap/forum/index.php';
                  }
                  else if(arr[1]=="mitbbs_article.php"){
                      if(!GetQueryString("opflag")) {
                          var board = GetQueryString("board");
                          var id = GetQueryString("id");
                          url = '/mwap/articlesingle.php?board='+board+'&article_id='+id;
                      }
                      else{
                          var board = GetQueryString("board");
                          var id = GetQueryString("id");
                          url = '/mwap/articlesingle.php?club='+board+'&article_id='+id;
                      }
                  }
                  else if(arr[1]=="mitbbs_article_t.php"){
                      if(!GetQueryString("opflag")) {
                          var board = GetQueryString("board");
                          var gid = GetQueryString("gid");
                          url = '/mwap/forum/article.php?board=' + board + '&groupid=' + gid + '&content_type=all';
                      }
                      else{
                          var board = GetQueryString("board");
                          var gid = GetQueryString("gid");
                          url = '/mwap/club/article.php?club=' + board + '&groupid=' + gid + '&content_type=all';
                      }
                  }
                  else if(arr[1]=="mitbbs_bbsdoc.php"){
                      var board = GetQueryString("board");
                      url = '/mwap/forum/board.php?board=' + board;
                  }
                  else if(arr[1]=="mitbbs_bbsboa.php"){
                      var group = GetQueryString("group");
